{
  "name": "sales-gestor-app",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "prebuild": "node scripts/generate-version.mjs",
    "build": "vite build",
    "prepare": "husky",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "format": "prettier --write \"**/*.{js,ts,svelte,html,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,svelte,html,css,json,md}\"",
    "cap:sync": "npx cap sync",
    "android:open": "npx cap open android",
    "android:copy": "npx cap copy android",
    "android:build": "npm run build && npx cap copy android && cd android && gradlew.bat assembleDebug",
    "android:build:debug": "npm run build && npx cap copy android && cd android && gradlew.bat assembleDebug",
    "android:build:release": "npm run build && npx cap copy android && cd android && gradlew.bat assembleRelease",
    "android:install:debug": "cd android && gradlew.bat installDebug",
    "android:install:release": "cd android && gradlew.bat installRelease",
    "android:clean": "cd android && gradlew.bat clean",
    "bump-version": "node scripts/bump-version.mjs"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/share": "^7.0.2",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.3",
    "tailwindcss": "^4.1.13"
  }
}
